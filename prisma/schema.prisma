generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model User {
  id Int @id @default(autoincrement())
  firstName String 
  lastName String
  username String @unique
  password String
  email String @unique
  phone String
  state String
  isAdminstrator Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
